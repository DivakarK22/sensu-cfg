{
  "rabbitmq": {
    "host": "127.0.0.1",
    "port": 5672,
    "user": "test",
    "password": "test",
    "vhost": "/"
  },
  "redis": {
    "host": "127.0.0.1",
    "port": 6379
  },
  "api": {
    "host": "127.0.0.1",
    "bind": "0.0.0.0",
    "port": 4567
  },
  "handlers": {
    "default": {
      "type": "set",
      "handlers": [
        "stdout"
      ]
    },
    "stdout": {
      "type": "pipe",
      "command": "cat"
    }
  },
  "checks": {
    "redis": {
      "command": "/opt/sensu/embedded/bin/check-process.rb -p redis",
      "subscribers": [
        "192.168.1.63"
      ],
      "interval": 10
    }
    "rabbitmq": {
      "command": "/opt/sensu/embedded/bin/check-process.rb -p rabbitmq-server",
      "subscribers": [
        "192.168.1.63"
      ],
      "interval": 10
    }
  },
  "client": {
    "name": "sensu-master",
    "address": "192.168.1.63",
    "subscriptions": [
      "test"
    ]
  }
}
