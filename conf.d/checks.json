{
    "checks": {
        "httpd": {
            "command": "/opt/sensu/embedded/bin/check-process.rb -p httpd",
            "handlers": [
                "default",
                "pushover"
            ],
            "interval": 180,
            "occurences": 3,
            "refresh": "1800",
            "standalone": true,
            "ttl": 1200
        },
        "mysqld": {
            "command": "/opt/sensu/embedded/bin/check-process.rb -p mysqld",
            "handlers": [
                "default",
                "pushover"
            ],
            "interval": 180,
            "occurences": 3,
            "refresh": "1800",
            "standalone": true,
            "ttl": 1200
        },
        "sensu": {
            "command": "/opt/sensu/embedded/bin/check-process.rb -p sensu-server",
            "handlers": [
                "default",
                "pushover"
            ],
            "interval": 30,
            "occurences": 3,
            "refresh": "1800",
            "standalone": true,
            "ttl": 1200
       },
       "redis": {
         "command": "/opt/sensu/embedded/bin/check-process.rb -p redis",
         "subscribers": [
           "192.168.1.63"
         ],
         "interval": 10
       }
  }
}
